<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>

<ResourceDictionary xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime"
                    xmlns:maui="clr-namespace:Microsoft.Maui;assembly=Microsoft.Maui">

    <system:Double x:Key="SpacingSmall">5</system:Double>
    <system:Double x:Key="SpacingMedium">10</system:Double>
    <system:Double x:Key="SpacingLarge">15</system:Double>

    <maui:Thickness x:Key="ThicknessSmall">5</maui:Thickness>
    <maui:Thickness x:Key="ThicknessMedium">10</maui:Thickness>
    <maui:Thickness x:Key="ThicknessLarge">15</maui:Thickness>

    <system:Double x:Key="ToolBarHeight">70</system:Double>

</ResourceDictionary>
